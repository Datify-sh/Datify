            "description": "Invalid request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden - no access to database"
          },
          "404": {
            "description": "Database not found"
          }
        },
        "security": [
          {
            "bearer": []
