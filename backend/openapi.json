  "components": {
    "schemas": {
      "AuthTokens": {
        "type": "object",
        "required": [
          "access_token",
          "refresh_token",
          "token_type",
          "expires_in"
        ],
        "properties": {
          "access_token": {
            "type": "string"
          },
          "expires_in": {
            "type": "integer",
            "format": "int64"
          },
          "refresh_token": {
            "type": "string"
          },
          "token_type": {
            "type": "string"
          }
        }
      },
      "CheckStatus": {
        "type": "object",
        "required": [
          "status"
        ],
        "properties": {
          "message": {
            "type": [
              "string",
              "null"
            ]
          },
          "status": {
            "type": "string"
          }
        }
      },
      "ConnectionInfo": {
        "type": "object",
        "required": [
          "host",
          "port",
          "username",
          "password",
          "database",
          "connection_string"
        ],
        "properties": {
          "connection_string": {
            "type": "string"
          },
          "database": {
            "type": "string"
          },
          "host": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "port": {
            "type": "integer",
            "format": "int32"
          },
          "username": {
            "type": "string"
          }
        }
      },
      "CreateDatabaseRequest": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "cpu_limit": {
            "type": "number",
            "format": "double"
          },
          "memory_limit_mb": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string"
          },
          "postgres_version": {
            "type": "string",
            "example": "16"
          },
          "storage_limit_mb": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "CreateProjectRequest": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "description": {
            "type": [
              "string",
              "null"
            ]
          },
          "name": {
            "type": "string"
          },
          "settings": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/Value"
              }
            ]
          }
        }
      },
      "DatabaseResponse": {
        "type": "object",
        "required": [
          "id",
          "project_id",
          "name",
          "postgres_version",
          "status",
          "resources",
          "created_at",
          "updated_at"
        ],
        "properties": {
          "connection": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "$ref": "#/components/schemas/ConnectionInfo"
              }
            ]
          },
          "created_at": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "postgres_version": {
            "type": "string"
          },
          "project_id": {
            "type": "string"
          },
          "resources": {
            "$ref": "#/components/schemas/ResourceLimits"
          },
          "status": {
            "type": "string"
          },
          "updated_at": {
