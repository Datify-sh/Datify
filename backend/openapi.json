              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LogsResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden - no access to database"
          },
          "404": {
            "description": "Database or container not found"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ]
      }
    },
